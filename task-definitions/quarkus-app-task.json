{
  "family": "quarkus-app-task",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "quarkus-app",
      "image": "${ECR_URI}/quarkus-app:latest",
      "memory": 512,
      "cpu": 256,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/quarkus-app",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::020157571320:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::020157571320:role/ecsTaskRole"
}
